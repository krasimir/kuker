{
  "name": "kuker",
  "version": "1.0.1",
  "description": "Debug like a boss",
  "main": "",
  "scripts": {
    "build-react-devtools-helpers": "browserify ./lib/react.devtools.helpers.src.js -o ./lib/react.devtools.helpers.js -t babelify",
    "build-content-script": "browserify ./lib/content.src.js -o ./lib/content.js -t babelify",
    "build": "git submodule update -f && npm run build-react-devtools-helpers && npm run build-content-script && browserify ./src/index.js -o ./lib/app.js -t babelify && rm ./build/* && node ./scripts/zipExtension.js",
    "watch-react-devtools-helpers": "watchify ./lib/react.devtools.helpers.src.js -o ./lib/react.devtools.helpers.js -t babelify -s reactDevToolsConnector -v",
    "watch-content-script": "watchify ./lib/content.src.js -o ./lib/content.js -t babelify -t brfs -v",
    "watch": "watchify ./src/index.js -o ./lib/app.js -t babelify -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krasimir/kuker.git"
  },
  "keywords": [
    "stent",
    "state",
    "machine",
    "dev",
    "tools",
    "chrome"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/kuker/issues"
  },
  "homepage": "https://github.com/krasimir/kuker",
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^14.4.0",
    "deep-diff": "^0.3.8",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "kuker-emitters": "6.3.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-json-tree": "^0.11.0",
    "react-virtualized": "^9.13.0",
    "stent": "4.1.0",
    "watchify": "^3.9.0"
  },
  "devDependencies": {
    "zip-folder": "^1.0.0"
  }
}
